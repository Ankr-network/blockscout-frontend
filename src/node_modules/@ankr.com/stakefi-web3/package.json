{
  "name": "@ankr.com/stakefi-web3",
  "version": "0.0.128",
  "description": "stkr js sdk to interact with stkr.io directly",
  "keywords": [
    "ankr",
    "ankr.com",
    "stkr",
    "stkr.io",
    "eth 2.0",
    "staking"
  ],
  "module": "dist",
  "typings": "dist/types",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "scripts": {
    "lint": "node_modules/.bin/tslint --project tsconfig.json",
    "prebuild": "rimraf dist",
    "build": "tsc",
    "precommit": "lint-staged; npm run test",
    "preversion": "npm run lint",
    "prepublish": "tsc",
    "publish": "yarn publish",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^14.14.41",
    "jest": "^27.2.5",
    "lerna": "^3.22.1",
    "ts-jest": "^27.0.6",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@ethereumjs/tx": "^3.3.2",
    "@types/node": "^14.14.41",
    "bignumber.js": "^9.0.1",
    "ethereumjs-util": "^7.1.3",
    "web3": "^1.3.5",
    "web3-eth-contract": "^1.3.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": ""
}

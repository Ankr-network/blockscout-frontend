{
  "name": "@ankr.com/multirpc",
  "version": "0.0.128",
  "description": "SDK for Multi RPC",
  "keywords": [
    "ankr",
    "ankr.com"
  ],
  "module": "dist",
  "typings": "dist/types",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "scripts": {
    "lint": "node_modules/.bin/tslint --project tsconfig.json",
    "prebuild": "rimraf dist",
    "build": "tsc && cpy '**/*' '!**/*.ts' ../dist/ --cwd=src/ --no-overwrite --parents",
    "precommit": "lint-staged; npm run test",
    "preversion": "npm run lint",
    "prepublish": "tsc && cpy '**/*' '!**/*.ts' ../dist/ --cwd=src/ --no-overwrite --parents",
    "publish": "yarn publish",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^14.14.41",
    "cpy-cli": "^3.1.1",
    "jest": "^27.2.5",
    "lerna": "^3.22.1",
    "ts-jest": "^27.0.6",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-config-standard": "^8.0.1"
  },
  "dependencies": {
    "@ankr.com/stakefi-web3": "^0.0.128",
    "@types/node": "^14.14.41",
    "bignumber.js": "^9.0.1",
    "ethereumjs-tx": "^2.1.2",
    "web3": "^1.3.5",
    "web3-eth-contract": "^1.3.5"
  },
  "directories": {
    "test": "test"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": ""
}

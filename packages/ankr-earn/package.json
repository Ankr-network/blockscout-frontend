{
  "name": "ankr-earn",
  "version": "2.1.0",
  "private": true,
  "homepage": "/earn",
  "dependencies": {
    "@binance-chain/bsc-connector": "^1.0.0",
    "@loadable/component": "^5.15.2",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@material-ui/pickers": "^3.3.10",
    "@redux-requests/axios": "^1.1.1",
    "@redux-requests/core": "^1.6.2",
    "@redux-requests/promise": "^1.0.2",
    "@redux-requests/react": "^1.7.0",
    "@reduxjs/toolkit": "^1.6.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@walletconnect/web3-provider": "^1.7.0",
    "axios": "^0.24.0",
    "bignumber.js": "^9.0.1",
    "classnames": "^2.3.1",
    "connected-react-router": "^6.9.1",
    "ethereumjs-tx": "^2.1.2",
    "final-form": "^4.20.6",
    "history": "4.10.1",
    "jss-preset-default": "^10.9.0",
    "limit-concurrency-decorator": "^0.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-final-form": "^6.5.7",
    "react-intl-universal": "^2.4.8",
    "react-redux": "^7.2.3",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-shallow-context": "^1.0.1",
    "react-uid": "^2.3.1",
    "redux-action": "^1.2.2",
    "redux-actions": "^2.6.5",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "redux-smart-actions": "^1.0.0",
    "typescript": "^4.1.2",
    "use-debounce": "^7.0.1",
    "web-vitals": "^1.0.1",
    "web3": "^1.6.1",
    "web3modal": "^1.9.4"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/node-logger": "^6.4.9",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.4.9",
    "@types/jest": "^26.0.15",
    "@types/loadable__component": "^5.13.4",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router": "^5.1.17",
    "@types/react-router-dom": "^5.3.2",
    "@types/redux-actions": "^2.6.2",
    "babel-loader": "8.1.0",
    "env-cmd": "^10.1.0",
    "lint-staged": "^12.1.2",
    "prettier": "^2.5.1",
    "react-app-rewired": "^2.1.8",
    "react-snap": "^1.23.0"
  },
  "scripts": {
    "start:dev": "env-cmd -f .env.dev react-app-rewired start",
    "start:stage": "env-cmd -f .env.stage react-app-rewired start",
    "start:prod": "env-cmd -f .env.prod react-app-rewired start",
    "start": "yarn start:stage",
    "build:dev": "env-cmd -f .env.dev react-app-rewired build",
    "build:stage": "env-cmd -f .env.stage react-app-rewired build",
    "build:prod": "env-cmd -f .env.prod react-app-rewired build",
    "test": "env-cmd react-app-rewired test",
    "eject": "react-app-rewired eject",
    "format": "lint-staged",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "postbuild": "react-snap"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.ts?(x)"
        ],
        "rules": {
          "@typescript-eslint/ban-types": [
            "error",
            {
              "types": {
                "React.FC": {
                  "message": "In favor of unification use the form \"(props: IComponentNameProp) => ReactNode\""
                }
              }
            }
          ],
          "@typescript-eslint/ban-ts-ignore": "off",
          "@typescript-eslint/no-empty-interface": "off",
          "@typescript-eslint/explicit-function-return-type": "off",
          "@typescript-eslint/no-explicit-any": "off",
          "@typescript-eslint/camelcase": "off",
          "@typescript-eslint/explicit-module-boundary-types": "off",
          "import/no-anonymous-default-export": "off"
        }
      },
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not ie <= 99",
      "not android <= 4.4.4",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "reactSnap": {
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ],
    "skipThirdPartyRequests": true,
    "inlineCss": true
  }
}

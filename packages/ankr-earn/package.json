  "scripts": {
    "start:dev": "concurrently \"env-cmd -f .env.dev react-scripts start\" \"yarn watch:dependencies\"",
    "start:stage": "concurrently \"env-cmd -f .env.stage react-scripts start\" \"yarn watch:dependencies\"",
    "start:prod": "concurrently \"env-cmd -f .env.prod react-scripts start\" \"yarn watch:dependencies\"",
    "start:dev:turbo": "cd ../.. && turbo run start:dev --filter=ankr-earn",
    "start:stage:turbo": "cd ../.. && turbo run start:stage --filter=ankr-earn...",
    "start:prod:turbo": "cd ../.. && turbo run start:prod --filter=ankr-earn...",
    "start": "yarn start:dev:turbo",
    "build:dev": "env-cmd -f .env.dev react-scripts build",
    "build:stage": "env-cmd -f .env.stage react-scripts build",
    "build:prod": "env-cmd -f .env.prod react-scripts build",
    "build:dev:turbo": "cd ../.. && turbo run build:dev --filter=ankr-earn...",
    "build:stage:turbo": "cd ../.. && turbo run build:stage --filter=ankr-earn...",
    "build:prod:turbo": "cd ../.. && turbo run build:prod --filter=ankr-earn...",
    "test": "env-cmd react-scripts test",
    "test:coverage": "yarn test --coverage",
    "test:turbo": "cd ../.. && turbo run test --filter=ankr-earn",
    "test:coverage:turbo": "cd ../.. && turbo run test:coverage --filter=ankr-earn",
    "types": "tsc --noEmit -p tsconfig.json",
    "lint:js": "cross-env NODE_ENV=production eslint src --ext=ts,tsx,js,jsx",
    "lint:js:fix": "yarn lint:js --fix",
    "format": "lint-staged",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "postbuild": "react-snap",
    "clean": "rimraf build dist",
    "watch:dependencies": "cd ../.. && turbo run watch --parallel --filter=ankr-earn..."
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "cross-env NODE_ENV=production eslint --ext=ts,tsx,js,jsx --fix"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "d3": "<rootDir>/../../node_modules/d3/dist/d3.min.js"
    },
    "coverageReporters": [
      "json-summary",
      "text",
      "text-summary"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not ie <= 99",
      "not android <= 4.4.4",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "reactSnap": {
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ],
    "include": [
      "/calc/",
      "/bridge/",
      "/liquid-crowdloan/",
      "/stake/matic/eth/",
      "/stake/matic/polygon/",
      "/unstake/avax/",
      "/unstake/bnb/",
      "/unstake/dot/",
      "/unstake/fantom/",
      "/unstake/ksm/",
      "/unstake/matic/eth/",
      "/unstake/matic/polygon/",
      "/ankr-stake/providers/",
      "/mgno-stake/providers/"
    ],
    "skipThirdPartyRequests": true,
    "inlineCss": true
  }
}

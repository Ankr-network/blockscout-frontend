  "scripts": {
    "start:dev": "env-cmd -f .env.dev react-app-rewired start",
    "start:stage": "env-cmd -f .env.stage react-app-rewired start",
    "start:prod": "env-cmd -f .env.prod react-app-rewired start",
    "start": "yarn start:stage",
    "build:dev": "env-cmd -f .env.dev react-app-rewired build",
    "build:stage": "env-cmd -f .env.stage react-app-rewired build",
    "build:prod": "env-cmd -f .env.prod react-app-rewired build",
    "test": "env-cmd react-app-rewired test",
    "test:coverage": "yarn test --coverage",
    "types": "tsc --noEmit -p tsconfig.json",
    "lint:js": "cross-env NODE_ENV=production eslint src --ext=ts,tsx,js,jsx",
    "lint:js:fix": "yarn lint:js --fix",
    "format": "lint-staged",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "postbuild": "react-snap"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "cross-env NODE_ENV=production eslint --ext=ts,tsx,js,jsx --fix"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not ie <= 99",
      "not android <= 4.4.4",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "reactSnap": {
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ],
    "include": [
      "/bridge/",
      "/liquid-crowdloan/",
      "/unstake/avax/",
      "/unstake/bnb/",
      "/unstake/fantom/",
      "/unstake/matic/"
    ],
    "skipThirdPartyRequests": true,
    "inlineCss": true
  }
}

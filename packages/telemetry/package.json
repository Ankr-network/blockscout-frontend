{
  "name": "@ankr.com/telemetry",
  "version": "0.1.1",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./src/index.ts",
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "ankr",
    "ankr.com"
  ],
  "license": "MIT",
  "private": false,
  "scripts": {
    "format": "lint-staged",
    "lint:js": "eslint \"./src/**/*.{js,ts,jsx,tsx}\"",
    "lint:js:fix": "yarn lint:js --fix --debug",
    "build": "vite build",
    "build:turbo": "cd ../.. && turbo run build --filter=@ankr.com/telemetry...",
    "types": "tsc --noEmit -p tsconfig.json",
    "watch": "yarn build --watch"
  },
  "dependencies": {
    "@ankr.com/ui": "*",
    "react-custom-scrollbars": "^4.2.1",
    "@types/react-custom-scrollbars": "4.0.10",
    "react-csv": "^2.2.2",
    "@types/react-csv": "^1.1.3"
  },
  "peerDependencies": {
    "react": ">=17.x <=18.x",
    "react-dom": ">=17.x <=18.x",
    "bignumber.js": "^9.1.2",
    "@mui/material": "5.x",
    "react-intl-universal": "^2.4.5",
    "recharts": "^2.8.0",
    "tss-react": "^4.8.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "rimraf": "3.0.2"
  },
  "lint-staged": {
    "*.{ts,tsx,json}": [
      "prettier --write"
    ]
  }
}

{
  "name": "ankr-protocol-backoffice",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start:stage": "env-cmd -f .env.stage react-scripts start",
    "start:prod": "env-cmd -f .env.prod react-scripts start",
    "start:stage:turbo": "cd ../.. && turbo run start:stage --parallel --filter=ankr-protocol-backoffice...",
    "start:prod:turbo": "cd ../.. && turbo run start:prod --parallel --filter=ankr-protocol-backoffice...",
    "build:stage": "env-cmd -f .env.stage react-scripts build",
    "build:prod": "env-cmd -f .env.prod react-scripts build",
    "build:stage:turbo": "cd ../.. && turbo run build:stage --filter=ankr-protocol-backoffice...",
    "build:prod:turbo": "cd ../.. && turbo run build:prod --filter=ankr-protocol-backoffice...",
    "test": "react-scripts test",
    "test:turbo": "cd ../.. && turbo run test --parallel --filter=ankr-protocol-backoffice...",
    "eject": "react-scripts eject",
    "format": "lint-staged",
    "lint:js": "eslint './src/**/*.{js,ts,jsx,tsx}'",
    "check-circular-dependency": "madge --circular src"
  },
  "dependencies": {
    "@ankr.com/provider": "*",
    "multirpc-sdk": "*",
    "common": "*",
    "@ant-design/icons": "^4.6.4",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "antd": "^4.16.13",
    "async-mutex": "0.3.2",
    "axios": "^0.21.4",
    "bignumber.js": "^9.0.1",
    "mobx": "^6.3.3",
    "mobx-react": "^7.2.0",
    "mobx-utils": "^6.0.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "typescript": "^4.1.2",
    "uuid": "^8.3.2",
    "web-vitals": "^1.0.1",
    "web3": "^1.5.2",
    "web3modal": "^1.9.4",
    "web3-eth-contract": "^1.6.0"
  },
  "devDependencies": {
    "@types/react-router-dom": "^5.1.8",
    "@types/uuid": "^8.3.1",
    "env-cmd": "^10.1.0",
    "prettier": "^2.5.1"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "cross-env NODE_ENV=production eslint --ext=ts,tsx,js,jsx --fix"
    ]
  },
  "browserslist": {
    "production": [
      "chrome >= 67",
      "edge >= 79",
      "firefox >= 68",
      "opera >= 54",
      "safari >= 14"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
    "react-custom-scrollbars": "^4.2.1",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.2.0",
    "react-virtualized": "9.22.3"
  },
  "devDependencies": {
    "@storybook/react": "^6.4.9",
    "@vitejs/plugin-react": "^2.1.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "vite": "^3.1.5",
    "vite-plugin-svgr": "^2.2.1",
    "rimraf": "3.0.2",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"

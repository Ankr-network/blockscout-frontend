    "ts-prune": "^0.10.3",
    "typescript": "^5.4.5"
  },
  "lint-staged": {
    "*.{ts,tsx,json}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "reactSnap": {
    "concurrency": 4,
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ],
    "include": [
      "/dashboard/",
      "/account/",
      "/account/success/",
      "/account/failure/",
      "/settings/",
      "/settings/confirmation",
      "/settings/invitation/",
      "/teams/new/",
      "/oauth/",
      "/enterprise/",
      "/projects/",
      "/projects/new/",
      "/referrals/"
    ],
    "inlineCss": true
  }

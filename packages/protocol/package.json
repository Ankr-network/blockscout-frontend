{
  "name": "@ankr.com/Web3APIApplication",
  "version": "1.75.0",
  "private": true,
  "scripts": {
    "start:stage": "concurrently \"env-cmd -f .env.stage react-scripts start\" \"yarn watch:dependencies\"",
    "start:prod": "concurrently \"env-cmd -f .env.prod react-scripts start\" \"yarn watch:dependencies\"",
    "start:stage:turbo": "cd ../.. && turbo run start:stage --filter=@ankr.com/Web3APIApplication...",
    "start:prod:turbo": "cd ../.. && turbo run start:prod --filter=@ankr.com/Web3APIApplication...",
    "build:stage": "env-cmd -f .env.stage react-scripts build",
    "build:prod": "env-cmd -f .env.prod react-scripts build",
    "build:stage:turbo": "cd ../.. && turbo run build:stage --filter=@ankr.com/Web3APIApplication...",
    "build:prod:turbo": "cd ../.. && turbo run build:prod --filter=@ankr.com/Web3APIApplication...",
    "eject": "react-scripts eject",
    "format": "lint-staged",
    "pre-commit": "npm run lint",
    "types": "tsc -p tsconfig.json",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "postbuild": "react-snap",
    "explore-bundles": "for file in build/static/js/*.js; do source-map-explorer \"$file\"; done",
    "clean": "rimraf build dist",
    "watch:dependencies": "cd ../.. && turbo run watch --parallel --filter=@ankr.com/Web3APIApplication...",
    "find-deadcode": "ts-prune"
  },
  "homepage": "/rpc",
  "dependencies": {
    "@ankr.com/assets": "^0.0.6",
    "@ankr.com/chains-list": "*",

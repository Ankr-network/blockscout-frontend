{
  "name": "ankr-protocol-web",
  "version": "1.6.4",
  "private": true,
  "scripts": {
    "start:stage": "env-cmd -f .env.stage react-scripts start",
    "start:prod": "env-cmd -f .env.prod react-scripts start",
    "start:stage:turbo": "cd ../.. && turbo run start:stage --parallel --filter=ankr-protocol-web...",
    "start:prod:turbo": "cd ../.. && turbo run start:prod --parallel --filter=ankr-protocol-web...",
    "build:stage": "env-cmd -f .env.stage react-scripts build",
    "build:prod": "env-cmd -f .env.prod react-scripts build",
    "build:stage:turbo": "cd ../.. && turbo run build:stage --filter=ankr-protocol-web...",
    "build:prod:turbo": "cd ../.. && turbo run build:prod --filter=ankr-protocol-web...",
    "test": "react-scripts test",
    "test:turbo": "cd ../.. && turbo run test --parallel --filter=ankr-protocol-web...",
    "eject": "react-scripts eject",
    "format": "lint-staged",
    "lint:js": "eslint \"./src/**/*.{js,ts,jsx,tsx}\"",
    "lint:js:fix": "yarn lint:js --fix",
    "types": "tsc --noEmit -p tsconfig.json",
    "check-circular-dependency": "madge --circular src",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "postbuild": "react-snap",
    "explore-bundles": "for file in build/static/js/*.js; do source-map-explorer \"$file\"; done"
  },
  "homepage": "/rpc",
  "dependencies": {
    "@ankr.com/global-menu": "*",
    "@ankr.com/provider": "*",
    "@loadable/component": "^5.14.1",
    "@material-ui/core": "4.11.0",
    "@material-ui/lab": "4.0.0-alpha.57",
    "@material-ui/pickers": "^3.3.10",
    "@material-ui/styles": "^4.11.4",
    "@react-hook/hover": "4.0.0",
    "@redux-requests/axios": "^1.1.1",
    "@redux-requests/core": "1.6.1",
    "@redux-requests/promise": "1.0.2",
    "@redux-requests/react": "1.6.0",
    "@reduxjs/toolkit": "^1.5.1",
    "@sentry/react": "^6.16.0",
    "@sentry/tracing": "^6.16.0",
    "@solana/web3.js": "^1.62.1",
    "@walletconnect/web3-provider": "^1.6.5",
    "async-mutex": "0.3.2",
    "avalanche": "3.15.4",
    "axios": "^0.21.1",
    "bignumber.js": "^9.0.1",
    "bn.js": "5.2.1",
    "bowser": "^2.11.0",
    "classnames": "^2.3.1",
    "common": "*",
    "connected-react-router": "^6.9.1",
    "country-iso-to-coordinates": "^1.0.2",
    "date-fns": "^2.28.0",
    "ethers": "5.7.0",
    "final-form": "^4.20.2",
    "final-form-arrays": "3.0.2",
    "final-form-set-field-touched": "^1.0.1",
    "history": "4.10.1",
    "immer": "^9.0.1",
    "is-valid-domain": "0.1.6",
    "json5": "^2.2.1",
    "lodash.isequal": "4.5.0",
    "near-api-js": "^1.1.0",
    "prism-react-renderer": "^1.3.3",
    "react": "^17.0.2",
    "react-copy-to-clipboard": "^5.0.3",
    "react-country-flag": "^2.3.1",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^17.0.2",
    "react-final-form": "^6.5.3",
    "react-final-form-arrays": "3.1.3",
    "react-intl-universal": "^2.4.5",
    "react-json-view": "^1.21.3",
    "react-parallax": "3.5.1",
    "react-redux": "^7.2.3",
    "react-router": "5.2.1",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3",
    "react-simple-maps": "^2.3.0",
    "react-uid": "^2.3.1",
    "recharts": "^2.1.4",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "redux-smart-actions": "^1.0.0",
    "uuid": "^9.0.0",
    "valid-url": "1.0.9",
    "web-vitals": "^1.0.1",
    "web3": "^1.3.5",
    "web3-core": "^1.6.1",
    "web3modal": "^1.9.4"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.3.8",
    "@storybook/addon-essentials": "^6.3.8",
    "@storybook/addon-links": "^6.3.8",
    "@storybook/node-logger": "^6.3.8",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.8",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/final-form-set-field-touched": "^1.0.0",
    "@types/history": "^4.7.9",
    "@types/jest": "^26.0.15",
    "@types/loadable__component": "^5.13.3",
    "@types/lodash.isequal": "4.5.6",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-copy-to-clipboard": "^4.3.0",
    "@types/react-custom-scrollbars": "^4.0.9",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-simple-maps": "^1.0.6",
    "@types/valid-url": "1.0.3",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "babel-loader": "8.1.0",
    "customize-cra": "^1.0.0",
    "env-cmd": "^10.1.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-typescript": "^0.14.0",
    "eslint-webpack-plugin": "^2.5.2",
    "lint-staged": "^10.5.4",
    "madge": "^5.0.1",
    "prettier": "^2.2.1",
    "react-app-rewired": "2.1.8",
    "react-snap": "^1.23.0",
    "source-map-explorer": "^2.5.2",
    "typescript": "^4.1.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not ie <= 99",
      "not android <= 4.4.4",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "reactSnap": {
    "concurrency": 1,
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ],
    "include": [
      "/account/",
      "/account/topup/",
      "/account/withdraw/",
      "/account/success/",
      "/account/failure/",
      "/settings/confirmation"
    ],
    "inlineCss": true
  }
}

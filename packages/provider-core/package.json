{
  "name": "@ankr.com/provider-core",
  "version": "0.6.3",
  "description": "Provider core module",
  "private": true,
  "main": "./dist/index.js",
  "types": "./src/index.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "ankr",
    "ankr.com"
  ],
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "precommit": "lint-staged; npm run test",
    "test": "jest --coverage --passWithNoTests",
    "test:watch": "jest --coverage --watch --passWithNoTests",
    "test:turbo": "cd ../.. && turbo run test --filter=provider-core",
    "test:watch:turbo": "cd ../.. && turbo run test:watch --filter=provider-core",
    "types": "tsc --noEmit -p tsconfig.json",
    "build": "vite build",
    "build:turbo": "cd ../.. && turbo run build --filter=provider-core...",
    "clean": "rimraf build dist",
    "watch": "yarn build --watch"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-gtm-module": "^2.0.1",
    "@types/react-router": "^5.1.17",
    "@types/react-router-dom": "^5.3.2",
    "cpy-cli": "^3.1.1",
    "eslint-config-prettier": "^7.2.0",
    "jest": "27.0.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "ts-jest": "27.0.0",
    "ts-node": "^7.0.1",
    "typescript": "^4.1.2",
    "rimraf": "3.0.2"
  },
  "dependencies": {
    "bignumber.js": "^9.0.1",
    "common": "*",
    "react": "^17.0.2",
    "web3": "1.7.0",
    "web3-core": "^1.7.0",
    "web3-eth-contract": "1.7.0",
    "web3-utils": "^1.3.5",
    "web3modal": "^1.9.4"
  },
  "directories": {
    "test": "test"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": ""
}

{
  "name": "@ankr.com/chains-list",
  "version": "1.0.1",
  "description": "Chains list module",
  "main": "./dist/index.js",
  "types": "./src/index.ts",
  "type": "module",
  "files": [
    "src",
    "dist"
  ],
  "keywords": [
    "ankr",
    "ankr.com"
  ],
  "license": "MIT",
  "private": false,
  "scripts": {
    "format": "lint-staged",
    "lint:js": "eslint \"./src/**/*.{js,ts,jsx,tsx}\"",
    "lint:js:fix": "yarn lint:js --fix --debug",
    "prebuild": "rimraf dist",
    "precommit": "lint-staged; yarn run test",
    "test": "jest --coverage --passWithNoTests",
    "test:watch": "jest --coverage --watch --passWithNoTests",
    "test:turbo": "cd ../.. && turbo run test --filter=@ankr.com/chains-list",
    "test:watch:turbo": "cd ../.. && turbo run test:watch --filter=@ankr.com/chains-list",
    "types": "tsc --noEmit -p tsconfig.json",
    "build": "vite build",
    "build:turbo": "cd ../.. && turbo run build --filter=@ankr.com/chains-list...",
    "clean": "rimraf build dist",
    "watch": "yarn build --watch"
  },
  "lint-staged": {
    "*.{ts,tsx,json}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint"
    ]
  },
  "directories": {
    "test": "test"
  },
  "dependencies": {},
  "peerDependencies": {
    "bignumber.js": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^22.5.4",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "babel-loader": "^9.1.3",
    "eslint": "^8.45.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^8.8.0",
    "lint-staged": "^10.5.4",
    "path": "^0.12.7",
    "prettier": "^2.2.1",
    "rimraf": "3.0.2",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "vite": "5.2.6"
  }
}

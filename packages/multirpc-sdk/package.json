{
  "name": "multirpc-sdk",
  "version": "0.6.3",
  "description": "SDK for Multi RPC",
  "private": true,
  "keywords": [
    "ankr",
    "ankr.com"
  ],
  "license": "MIT",
  "scripts": {
    "lint": "eslint './src/**/*.{js,ts,jsx,tsx}'",
    "prebuild": "rimraf dist",
    "build": "tsc && cpy '**/*' '!**/*.ts' ../dist/ --cwd=src/ --no-overwrite --parents",
    "precommit": "lint-staged; npm run test",
    "preversion": "npm run lint",
    "prepublish": "tsc && cpy '**/*' '!**/*.ts' ../dist/ --cwd=src/ --no-overwrite --parents",
    "publish": "yarn publish",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "generate": "abi-types-generator ./src/PAYGContract/abi/PayAsYouGo.json --name=IPayAsYouGo --provider=web3"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^14.14.41",
    "cpy-cli": "^3.1.1",
    "eslint-config-prettier": "^7.2.0",
    "jest": "^27.2.5",
    "lerna": "^3.22.1",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.2",
    "ts-node": "^7.0.1"
  },
  "dependencies": {
    "@ankr.com/stakefi-web3": "^0.0.152",
    "@faker-js/faker": "6.1.2",
    "@types/lodash.orderby": "4.6.6",
    "@types/node": "^14.14.41",
    "@types/uuid": "8.3.4",
    "axios": "^0.21.1",
    "bignumber.js": "^9.0.1",
    "bn.js": "5.2.0",
    "ethereum-abi-types-generator": "1.3.2",
    "ethereumjs-tx": "^2.1.2",
    "lodash.orderby": "4.6.0",
    "qs": "^6.10.3",
    "typescript": "^4.1.2",
    "uuid": "8.3.2",
    "web3": "^1.3.5",
    "web3-core": "1.6.1",
    "web3-eth-contract": "^1.3.5",
    "web3-utils": "^1.7.3"
  },
  "directories": {
    "test": "test"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": ""
}
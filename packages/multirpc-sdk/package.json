{
  "name": "multirpc-sdk",
  "version": "0.8.0",
  "description": "SDK for Multi RPC",
  "main": "./dist/index.js",
  "types": "./src/index.ts",
  "files": [
    "dist"
  ],
  "private": true,
  "keywords": [
    "ankr",
    "ankr.com"
  ],
  "license": "MIT",
  "scripts": {
    "types": "tsc --noEmit -p tsconfig.json",
    "lint:js": "eslint \"./src/**/*.{js,ts,jsx,tsx}\"",
    "lint:js:fix": "yarn lint:js --fix --debug",
    "prebuild": "rimraf dist",
    "build": "vite build",
    "build:turbo": "cd ../.. && turbo run build --filter=multirpc-sdk...",
    "precommit": "lint-staged; npm run test",
    "preversion": "npm run lint",
    "prepublish": "tsc && cpy '**/*' '!**/*.ts' ../dist/ --cwd=src/ --no-overwrite --parents",
    "publish": "yarn publish",
    "test": "jest --coverage --passWithNoTests",
    "test:turbo": "cd ../.. && turbo run test --filter=multirpc-sdk",
    "test:watch": "jest --coverage --watch --passWithNoTests",
    "test:watch:turbo": "cd ../.. && turbo run test:watch --filter=multirpc-sdk",
    "generate": "abi-types-generator ./src/PAYGContract/abi/PayAsYouGo.json --name=IPayAsYouGo --provider=web3",
    "clean": "rimraf build dist",
    "watch": "yarn build --watch"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "yarn lint:js:fix",
      "git add"
    ]
  },
  "dependencies": {
    "@ankr.com/advanced-api": "^1.1.0",
    "@ankr.com/provider": "^1.10.2",
    "@ankr.com/common": "*",
    "@ankr.com/utils": "*",
    "axios": "^0.21.1",
    "bignumber.js": "^9.0.1",
    "bn.js": "5.2.0",
    "ethereum-abi-types-generator": "1.3.2",
    "ethereumjs-util": "^7.0.7",
    "eth-sig-util": "^3.0.1",
    "lodash.flatten": "4.4.0",
    "lodash.orderby": "4.6.0",
    "qs": "^6.10.3",
    "typescript": "^4.1.2",
    "uuid": "8.3.2",
    "web3": "1.7.0",
    "web3-core": "1.7.0",
    "web3-eth-contract": "1.7.0",
    "web3-utils": "1.7.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@types/lodash.flatten": "4.4.7",
    "@types/lodash.orderby": "4.6.6",
    "@types/node": "^14.14.41",
    "@types/uuid": "8.3.4",
    "eslint-config-prettier": "^7.2.0",
    "prettier": "^2.2.1",
    "ts-node": "^7.0.1",
    "rimraf": "3.0.2",
    "source-map-explorer": "^2.5.2"
  },
  "directories": {
    "test": "test"
  },
  "publishConfig": {
    "access": "public"
  }
}
{
  "name": "admin-mrpc",
  "version": "1.0.5",
  "private": true,
  "scripts": {
    "start:stage": "concurrently \"env-cmd -f .env.stage react-scripts start\" \"yarn watch:dependencies\"",
    "start:prod": "concurrently \"env-cmd -f .env.prod react-scripts start\" \"yarn watch:dependencies\"",
    "start:stage:turbo": "cd ../.. && turbo run start:stage --filter=admin-mrpc...",
    "start:prod:turbo": "cd ../.. && turbo run start:prod --filter=admin-mrpc...",
    "build:stage": "env-cmd -f .env.stage react-scripts build",
    "build:prod": "env-cmd -f .env.prod react-scripts build",
    "build:stage:turbo": "cd ../.. && turbo run build:stage --filter=admin-mrpc...",
    "build:prod:turbo": "cd ../.. && turbo run build:prod --filter=admin-mrpc...",
    "postbuild": "react-snap",
    "test": "react-scripts test",
    "test:turbo": "cd ../.. && turbo run test --filter=admin-mrpc",
    "eject": "react-scripts eject",
    "types": "tsc --noEmit -p tsconfig.json",
    "format": "lint-staged",
    "lint": "eslint \"./src/**/*.{js,ts,jsx,tsx}\"",
    "lint:js": "cross-env NODE_ENV=production eslint src --ext=ts,tsx,js,jsx",
    "lint:js:fix": "yarn lint:js --fix",
    "clean": "rimraf build dist",
    "watch:dependencies": "cd ../.. && turbo run watch --parallel --filter=admin-mrpc..."
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not ie <= 99",
      "not android <= 4.4.4",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@ankr.com/global-menu": "*",
    "@ankr.com/provider": "*",
    "@ankr.com/staking-sdk": "*",
    "@ankr.com/ui": "0.3.5",
    "@ankr.com/utils": "^1.0.0",
    "@emotion/cache": "^11.10.3",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@fontsource/inter": "^4.5.13",
    "@mui/material": "^5.10.10",
    "@reduxjs/toolkit": "^1.8.5",
    "async-mutex": "^0.3.2",
    "axios": "^0.27.2",
    "bignumber.js": "^9.1.0",
    "classnames": "^2.3.1",
    "lodash.isequal": "^4.5.0",
    "multirpc-sdk": "*",
    "query-string": "^7.1.1",
    "react": "^17.0.2",
    "react-copy-to-clipboard": "^5.1.0",
    "react-csv": "^2.2.2",
    "react-dom": "^17.0.2",
    "react-intl-universal": "^2.6.6",
    "react-redux": "^8.0.2",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-toastify": "^8.1.0",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.2.1",
    "redux-smart-actions": "^1.0.0",
    "tss-react": "^4.4.1",
    "typescript": "^4.1.2",
    "web-vitals": "^2.1.4",
    "web3modal": "^1.9.9"
  },
  "devDependencies": {
    "@storybook/react": "^6.3.8",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.56",
    "@types/react": "^17.0.0",
    "@types/react-copy-to-clipboard": "^5.0.4",
    "@types/react-csv": "^1.1.3",
    "@types/react-dom": "^17.0.0",
    "@types/react-router": "^5.1.17",
    "@types/react-router-dom": "^5.3.2",
    "@types/redux-actions": "^2.6.2",
    "concurrently": "^7.4.0",
    "eslint-webpack-plugin": "^3.2.0",
    "lint-staged": "^13.0.3",
    "react-app-rewired": "^2.2.1",
    "react-snap": "^1.23.0",
    "rimraf": "3.0.2"
  },
  "resolutions": {
    "react-devtools-core": "4.14.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "cross-env NODE_ENV=production eslint --ext=ts,tsx,js,jsx --fix"
    ]
  },
  "reactSnap": {
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ],
    "include": [
      "/clients"
    ],
    "skipThirdPartyRequests": true,
    "inlineCss": true
  }
}
